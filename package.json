{
  "name": "lionmiss-components",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "preview": "vite preview",
    "build": "tsc && vite build",
    "build:lib": "npm run clean:lib && npm run build:babel && npm run build:types && npm run build:style",
    "build:babel": "npx babel ./src/lib -d ./lib --extensions \".ts,.tsx\" --config-file ./config/.babelrc",
    "build:swc": "npx swc ./src/lib --config-file ./config/.swcrc --out-dir ./",
    "build:types": "npx tsc --project ./config/tsconfig.lib.json",
    "build:style": "npx node-sass ./src/lib --recursive --output ./lib",
    "clean:lib": "rm -rf ./lib",
    "eslint": "npx eslint --config ./.eslintrc.json ./src",
    "test": "vitest"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.3.0",
    "@fortawesome/fontawesome-svg-core": "^6.3.0",
    "@fortawesome/free-brands-svg-icons": "^6.3.0",
    "@fortawesome/free-regular-svg-icons": "^6.3.0",
    "@fortawesome/free-solid-svg-icons": "^6.3.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@js-joda/core": "^5.5.2",
    "classname": "^0.0.0",
    "i18next": "^22.4.12",
    "jsdom": "^21.1.1",
    "lionmiss-core": "^0.3.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^12.2.0",
    "reset-css": "^5.0.1",
    "rxjs": "^7.8.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.3",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.0",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.44",
    "@testing-library/react": "^14.0.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "babel-preset-minify": "^0.5.2",
    "eslint": "^8.36.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "node-sass": "^8.0.0",
    "storybook": "^7.0.0-rc.8",
    "typescript": "^5.0.2",
    "vite-plugin-eslint": "^1.8.1"
  }
}
