{
  "name": "lionmiss-components",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "npm run clean && npm run build:babel && npm run build:types && npm run build:style",
    "build:babel": "npx babel ./src -d ./lib --extensions \".ts,.tsx\" --config-file ./config/.babelrc",
    "build:swc": "npx swc ./src --config-file ./config/.swcrc --out-dir ./",
    "build:types": "npx tsc --project ./config/tsconfig.json",
    "style:build": "npx node-sass ./src --recursive --output ./lib",
    "clean": "rm -rf ./lib",
    "clean:disaster": "find . -type f -name \"*.d.ts\" -name \".js\" -name \".js.map\" -delete",
    "eslint": "npx eslint --config ./.eslintrc.json ./src",
    "test": "vitest",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.4.0",
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/free-brands-svg-icons": "^6.4.0",
    "@fortawesome/free-regular-svg-icons": "^6.4.0",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@js-joda/core": "^5.5.3",
    "classname": "^0.0.0",
    "classnames": "^2.3.2",
    "jsdom": "^21.1.1",
    "lionmiss-core": "^0.5.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "reset-css": "^5.0.1",
    "rxjs": "^7.8.0",
    "storybook": "^7.0.6",
    "styled-components": "^5.3.10"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.4",
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.4",
    "@storybook/addon-essentials": "^7.0.6",
    "@storybook/addon-interactions": "^7.0.6",
    "@storybook/addon-links": "^7.0.6",
    "@storybook/blocks": "^7.0.6",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^7.0.6",
    "@storybook/react-webpack5": "^7.0.6",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.53",
    "@testing-library/react": "^14.0.0",
    "@types/react": "^18.0.38",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "babel-preset-minify": "^0.5.2",
    "css-loader": "^6.7.3",
    "eslint": "^8.39.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-storybook": "^0.6.11",
    "node-sass": "^8.0.0",
    "prop-types": "^15.8.1",
    "sass": "^1.62.0",
    "sass-loader": "^13.2.2",
    "style-loader": "^3.3.2",
    "typescript": "^5.0.4"
  }
}
