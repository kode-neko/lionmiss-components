var useKNCarousel = function(imgList, width) {
    var _useState = _slicedToArray(useState(0), 2), index = _useState[0], _setIndex = _useState[1];
    var indexeRef = useRef(index);
    var setIndex = function(data) {
        indexeRef.current = data;
        _setIndex(data);
    };
    var _useState1 = _slicedToArray(useState({
        cont: width ? width : 100,
        units: width ? "px" : "vw"
    }), 2), imgWidth = _useState1[0], setImgWidth = _useState1[1];
    useEffect(function() {
        var mediaQueryList = window.matchMedia("(max-imgWidth: ".concat(createValueWidth(imgWidth), ")"));
        var listener = function(event) {
            var newWidth = {
                cont: imgWidth.cont,
                units: "px"
            };
            if (event.matches) newWidth = {
                cont: 100,
                units: "vw"
            };
            setImgWidth(newWidth);
            event.stopPropagation();
        };
        mediaQueryList.addEventListener("change", listener);
        return function() {
            mediaQueryList.removeEventListener("change", listener);
        };
    // eslint-disable-next-line react-hooks/exhaustive-deps
    }, []);
    var handleOnClickArrow = function(index) {
        var auxIndex = index;
        if (index > imgList.length - 1) auxIndex = 0;
        else if (index < 0) auxIndex = imgList.length - 1;
        setIndex(auxIndex);
    };
    var onClickLeft = function() {
        return handleOnClickArrow(index - 1);
    };
    var onClickRight = function() {
        return handleOnClickArrow(index + 1);
    };
    var onClickPage = function(index) {
        return handleOnClickArrow(index);
    };
    return [
        index,
        createValueWidth(imgWidth),
        onClickLeft,
        onClickRight,
        onClickPage
    ];
};
