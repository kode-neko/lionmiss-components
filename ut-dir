import { useState, useEffect } from "react";
function useDisplay(widthDisplay) {
    var _useState = _slicedToArray(useState(), 2), isResponsive = _useState[0], setIsResponsive = _useState[1];
    useEffect(function() {
        var mediaQueryList = window.matchMedia("(max-width: ".concat(widthDisplay, "px)"));
        var listener = function(event) {
            setIsResponsive(event.matches);
            event.stopPropagation();
        };
        mediaQueryList.addEventListener("change", listener);
        return function() {
            mediaQueryList.removeEventListener("change", listener);
        };
    // eslint-disable-next-line react-hooks/exhaustive-deps
    }, []);
    return isResponsive;
}
export default useDisplay;

