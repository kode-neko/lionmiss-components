var ONLY_NUMBERS = /\d/;
var ONLY_4_NUMBERS = /\d{4}$/;
var checkNumberKey = function(key) {
    return ONLY_NUMBERS.test(key);
};
var checkDeleteKey = function(key) {
    return [
        "Del",
        "Delete",
        "Backspace"
    ].includes(key);
};
var checkOnly4Digits = function(val) {
    return ONLY_4_NUMBERS.test(val);
};
export { checkNumberKey, checkDeleteKey, checkOnly4Digits };

function checkMaxMinStr(val, min, max) {
    var numVal = Number(val);
    if (numVal >= min && numVal <= max) return true;
    return false;
}
export default checkMaxMinStr;

function completeZerosStrNum(number, digits) {
    var numberZeros = digits - number.length;
    return Array(numberZeros).fill(0).concat(number).join("");
}
export default completeZerosStrNum;

var createPath = function(pathList) {
    return pathList.join("/");
};
export default createPath;

import { DateTimeFormatter, LocalDate } from "@js-joda/core";
var DATE_NOW = LocalDate.now();
var FORMAT_DATE_YY = "yy";
function getLastDigitsYear() {
    var date = arguments.length > 0 && arguments[0] !== void 0 ? arguments[0] : DATE_NOW;
    var formatter = DateTimeFormatter.ofPattern(FORMAT_DATE_YY);
    return Number(date.format(formatter));
}
export default getLastDigitsYear;

