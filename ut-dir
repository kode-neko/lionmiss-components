import { useEffect } from "react";
function useAddEvent(refElement, eventName, handler) {
    useEffect(function() {
        var ele = refElement.current;
        ele === null || ele === void 0 ? void 0 : ele.addEventListener(eventName, handler);
        return function() {
            ele === null || ele === void 0 ? void 0 : ele.removeEventListener(eventName, handler);
        };
    // eslint-disable-next-line react-hooks/exhaustive-deps
    }, []);
}
export default useAddEvent;

